lazysoap:
  listen_addr: 0.0.0.0:8080
  read_timeout: 10s
  read_header_timeout: 10s
  write_timeout: 10s
  idle_timeout: 60s
  gracefult_timeout: 5s
  img_client:
    timeout: 10s
    max_idle_conns: 100
    idle_conn_timeout: 60s

tmdb:
  api_key: "<required>"
  enable_auto_retry: true
  max_idle_conns: 100
  idle_conn_timeout: 60s
  request_timeout: 10s

trace:
  ratio: 1.0
  gracefult_timeout: 5s
  endpoint: http://127.0.0.1:14268/api/traces
